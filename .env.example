# TeleChat Docker Environment Configuration
# Copy this file to .env and modify as needed

# Model configuration
MODEL_PATH=/app/models/7B

# GPU configuration
# Use specific GPUs: 0,1,2,3
# Use all GPUs: leave as 0 or set to multiple numbers
# Use CPU only: set to empty string
CUDA_VISIBLE_DEVICES=0

# API Service Port
API_PORT=8070

# Web Service Port
WEB_PORT=8501
